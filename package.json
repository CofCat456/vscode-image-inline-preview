{
  "publisher": "cofcat",
  "name": "image-inline-preview",
  "displayName": "Image Inline Preview",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "description": "",
  "author": "CofCat <a25690756@gmail.com.tw>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/cofcat",
  "homepage": "https://github.com/cofcat/vscode-image-inline-preview#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cofcat/vscode-image-inline-preview"
  },
  "bugs": {
    "url": "https://github.com/cofcat/vscode-image-inline-preview/issues"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Image Inline Preview",
      "properties": {
        "image-inline-preview.languageIds": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact",
            "vue",
            "svelte",
            "html",
            "pug",
            "json",
            "yaml"
          ],
          "description": "Array of language IDs to enable annotations"
        },
        "image-inline-preview.annotations": {
          "type": "boolean",
          "default": true,
          "description": "Enabled Image inline annotations"
        },
        "image-inline-preview.position": {
          "type": "string",
          "enum": [
            "before",
            "after"
          ],
          "default": "before",
          "description": "Position the image before or after the text"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "pnpm run update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.4",
    "@antfu/ni": "^0.22.0",
    "@types/node": "^20.14.11",
    "@types/vscode": "^1.91.0",
    "@vscode/vsce": "^2.30.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "pnpm": "^9.5.0",
    "reactive-vscode": "0.2.0-beta.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3",
    "vscode-ext-gen": "^0.3.2"
  }
}
